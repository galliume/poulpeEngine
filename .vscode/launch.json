{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "RBKDebug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\Rebulkan.exe",
            "symbolSearchPath": "${workspaceFolder}\\build\\Rebulkan.pdb",
            "cwd": "${workspaceFolder}\\build\\",
            "console": "externalTerminal",
            "logging": {
              "moduleLoad": false,
              "trace": true
            }
        }
    ]
}